@model IEnumerable<RedisStudy.DAL.Abstraction.Models.Function>

@{
    ViewBag.Title = "函数列表";
}

<h2>函数列表</h2>

<p>
    @Html.ActionLink("新增", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FunName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FunType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FunRemark)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsUsed)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FunName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FunType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FunRemark)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsUsed)
        </td>
        <td>
            @Html.ActionLink("编辑", "Edit", new { id=item.Id }) |
            @Html.ActionLink("详细", "Details", new { id=item.Id }) |
            @Html.ActionLink("删除", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
